<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">


            <item android:bottom="1.3dp" android:left="1.3dp" android:right="1.3dp" android:top="1.3dp">
                <shape android:shape="rectangle">
                    <stroke android:width="1.3dp" android:color="#000000" />
                    <corners android:radius="10dp"></corners>
                    <solid android:color="#00000000" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="true">
        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">


            <item android:bottom="2.5dp" android:left="2.5dp" android:right="2.5dp" android:top="2.5dp">
                <shape android:shape="rectangle">
                    <stroke android:width="2.5dp" android:color="#C0C0C0" />
                    <corners android:radius="10dp"></corners>
                    <solid android:color="#00000000"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>